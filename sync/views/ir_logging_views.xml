<odoo>
    <record id="ir_logging_action" model="ir.actions.act_window">
        <field name="name">Logs</field>
        <field name="res_model">ir.logging</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[('sync_job_id', '!=', False)]</field>
